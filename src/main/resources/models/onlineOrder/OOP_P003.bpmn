    <userTask id="formTask1" name="Check Stock" flowable:assignee="${initiator}" flowable:dueDate="P5D" flowable:formKey="OOP_F004" flowable:formFieldValidation="false">
      <extensionElements>
        <flowable:sla-definition-key><![CDATA[OOP_SLA001]]></flowable:sla-definition-key>
        <flowable:task-candidates-type><![CDATA[all]]></flowable:task-candidates-type>
        <flowable:element-rendering exposeToCaseView="true" label="Check Stock" icon="stocking/regular" index="1"></flowable:element-rendering>
        <flowable:state-rendering active="fullAccess" completed="hidden"></flowable:state-rendering>
        <design:stencilid><![CDATA[FormTask]]></design:stencilid>
        <design:stencilsuperid><![CDATA[Task]]></design:stencilsuperid>
      </extensionElements>
    </userTask>
    <serviceTask id="emailTask1" name="Inform Customer" flowable:type="mail">
      <extensionElements>
        <flowable:field name="to">
          <flowable:expression><![CDATA[${emailAddress}]]></flowable:expression>
        </flowable:field>
        <flowable:field name="subject">
          <flowable:expression><![CDATA[Your order ${root.caseSequence} cannot be fullfilled]]></flowable:expression>
        </flowable:field>
        <flowable:field name="text">
          <flowable:expression><![CDATA[Hi ${root.firstName} ${root.lastName}
<br></br>
The ordered items are currently out of stock. Please check back in the near future.
<br></br>
Best regards,
Your OnlineShop]]></flowable:expression>
        </flowable:field>
        <design:stencilid><![CDATA[EmailTask]]></design:stencilid>
        <design:stencilsuperid><![CDATA[Task]]></design:stencilsuperid>
      </extensionElements>
    </serviceTask>
    <userTask id="formTask3" name="Send Package" flowable:assignee="${initiator}" flowable:dueDate="P3D" flowable:formKey="oOP_F007ShipPackageForm" flowable:formFieldValidation="false">
      <extensionElements>
        <flowable:sla-definition-key><![CDATA[OOP_SLA001]]></flowable:sla-definition-key>
        <flowable:task-candidates-type><![CDATA[all]]></flowable:task-candidates-type>
        <flowable:element-rendering exposeToCaseView="true" label="Send Package" icon="boxes-packing/regular" index="1"></flowable:element-rendering>
        <flowable:state-rendering active="fullAccess" completed="hidden"></flowable:state-rendering>
        <design:stencilid><![CDATA[FormTask]]></design:stencilid>
        <design:stencilsuperid><![CDATA[Task]]></design:stencilsuperid>
      </extensionElements>
    </userTask>
