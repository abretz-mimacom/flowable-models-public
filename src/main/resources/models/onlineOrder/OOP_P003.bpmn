        <design:stencilid><![CDATA[SequenceFlow]]></design:stencilid>
        <design:display_ref_in_diagram><![CDATA[true]]></design:display_ref_in_diagram>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${vars:equals(stockAvailable, 'No')}]]></conditionExpression>
    </sequenceFlow>
